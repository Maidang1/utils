{
  "name": "@maidang606/utils",
  "version": "0.0.3",
  "description": "Common utility functions",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "utils"
  ],
  "author": "maidang@felixwliu.cn",
  "license": "MIT",
  "homepage": "https://github.com/Maidang1/utils",
  "repository": {
    "url": "git+https://github.com/Maidang1/utils.git"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist",
    "package.json",
    "readme.md",
    "tsconfig.json"
  ],
  "dependencies": {
    "youch": "^3.3.3",
    "youch-terminal": "^2.2.3"
  }
}
